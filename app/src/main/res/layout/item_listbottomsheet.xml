<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <data>
        <variable
            name="text"
            type="String" />

    </data>

    <LinearLayout
        android:id="@+id/linearlayout_listbottomsheet_container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <TextView
            android:id="@+id/textview_listbottomsheet_text"
            style="@style/SellerStoreMenuTextStyle"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="@color/black_four"
            android:text="@{text}" />
    </LinearLayout>
</layout>